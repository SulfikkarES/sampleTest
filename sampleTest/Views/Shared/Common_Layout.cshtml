<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/css/sales-fonts.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700;900&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    <script type="text/javascript" src="~/js/common.js"></script>

</head>
<body>
    <div class="nav">
        <div class="res-menu">
            <span class="ak-menu icon" data-target="slide-out" onclick="openNav()"></span>
        </div>
        <a class="logo" href="">
            <img src="~/images/main-logo.png" alt="">
        </a>
      
        <ul class="main-menu">
            <li><a href="@Url.Action("Profile","AdminLoginUpdated")">Home</a></li>

            <li><a href="@Url.Action("MedicalLeadAgentFormSave","MedicalLeadAgent")">Create</a></li>
            <li><a href="@Url.Action("MedicalLeadAgentForm","MedicalLeadAgent")">Report</a></li>
            <li><a href="@Url.Action("ForgotPassword","AdminLoginUpdated")">Change Password</a></li>
            <li><a href="@Url.Action("Logout","AdminLoginUpdated")">Logout</a></li>



            <!--<li><a href="">Works</a></li>-->
            <li>
                <a href="#" class="user-logo dropdown-trigger" onclick="myFunction1()">
                    <img src="~/images/img_avatar.png" alt="">
                </a>
            </li>
        </ul>



    </div>
    <div class="container" @*style="margin-top: 20px;"*@>
        @RenderBody()
        <hr />
        @*<div class="row">
            <footer>
                <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
            </footer>
        </div>*@
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
